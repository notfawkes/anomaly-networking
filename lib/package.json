{
  "name": "react-connect-lines",
  "version": "0.0.4",
  "description": "Connect elements with lines",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel . --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --ignore \"node_modules/**/*,dist/**/*\"",
    "preinstall": "echo Installing dependencies...",
    "publish": "npm run build && npm publish"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "rimraf": "^5.0.1",
    "typescript": "~4.8.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hermanwikner/react-connect-lines.git"
  },
  "keywords": [
    "react",
    "connect",
    "lines"
  ],
  "author": "Herman Wikner",
  "license": "MIT",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions"
  ],
  "bugs": {
    "url": "https://github.com/hermanwikner/react-connect-lines/issues"
  },
  "homepage": "https://github.com/hermanwikner/react-connect-lines#readme"
}
