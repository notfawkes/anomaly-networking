{
  "name": "react-connect-lines",
  "version": "0.0.4",
  "description": "Connect elements with lines",
  "scripts": {
    "build": "parcel build --no-cache",
    "watch": "rm -rf dist && parcel watch",
    "preinstall": "echo Installing dependencies...",
    "publish": "yarn build && npm publish"
  },
  "source": "./index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "devDependencies": {
    "parcel": "2.6.2",
    "@parcel/packager-ts": "2.6.2",
    "@parcel/transformer-typescript-types": "2.6.2",
    "typescript": ">=3.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hermanwikner/react-connect-lines.git"
  },
  "keywords": [
    "react",
    "connect",
    "lines"
  ],
  "author": "Herman Wikner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hermanwikner/react-connect-lines/issues"
  },
  "homepage": "https://github.com/hermanwikner/react-connect-lines#readme"
}
